println(x);#10.000000

struct Vec2
{
    var length=function(x,y)
    {
        return x*x+y*y;
    };
} 
struct Vec3
{
    var super=Vec2;
} 
var a=Vec3;
var b=a.super.length(10,8);
println(b);    #164.000000
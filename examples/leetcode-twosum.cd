var nums=[2,7,11,15];
var target=9;

function twosum(nums,target)
{
    var i=0;
    var j=i+1;

    while(i<sizeof(nums)-1)
    {
        println(i);
        println(j);
        j=i+1;
        while(j<sizeof(nums))
        {
            println(i);
            println(j);
            if(nums[i]+nums[j]==target)
                return [i,j];
            j=j+1;
        }
        i=i+1;
    }
}

println(twosum(nums,target));#[0.000000,1.000000]
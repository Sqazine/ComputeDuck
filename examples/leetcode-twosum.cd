nums=[2,7,11,15];
target=9;

twosum=function(nums,target)
{
    i=0;
    j=i+1;

    while(i<sizeof(nums)-1)
    {
        j=i+1;
        while(j<sizeof(nums))
        {
            if(nums[i]+nums[j]==target)
                return [i,j];
            j=j+1;
        }
        i=i+1;
    }
};

println(twosum(nums,target));#[0.000000,1.000000]